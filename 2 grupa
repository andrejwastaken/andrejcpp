#include <iostream>
#include <cstdlib>
#include <iomanip>
#include <string>
#include <iso646.h>
using namespace std;
int main()
{
int n,zbir,b;
zbir = 0;
b = 0;
cout<<"Vnesi do koj broj sakas: "<<endl;
cin>>n;
for(int i=1; i<=n ;i++)
if(i%5 == 0){
b++;
zbir = zbir + i*i ;

}
cout<<"Zbirot na kvadratite do "<<n<< " e "<<zbir<<". Ima " <<b<< " broevi delivi so 5"<<endl;
    system("PAUSE");
    return 0;

}

